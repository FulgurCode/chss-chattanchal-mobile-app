{
  "expo": {
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./imgs/icons/splash.png",
      "resizeMode": "contain"
    },
    "icon": "./imgs/icons/icon.png",
    "scheme": "chss",
    "web": {
      "bundler": "metro"
    },
    "name": "CHSS Mobile",
    "slug": "chss-chattanchal-mobile-app",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e22dcded-621c-4abf-acd3-b4c226224ec6"
      }
    },
    "android": {
      "userInterfaceStyle": "automatic",

      "package": "com.fulgurcode.chsschattanchalmobileapp",
      "adaptiveIcon": {
        "foregroundImage": "./imgs/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },

    "userInterfaceStyle": "automatic",
    "ios": {
      "userInterfaceStyle": "automatic",
      "bundleIdentifier" : "chss"
    }
  }
  
}



