{
  "expo": {
    "splash": {
      "backgroundColor": "#ffffff",
      "image": "./imgs/icons/splash.png",
      "resizeMode" : "contain"
    },
    "icon": "./imgs/icons/icon.png",
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "name": "chss-chattanchal-mobile-app",
    "slug": "chss-chattanchal-mobile-app",
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a63f791c-79a3-4907-b87e-cdf4646df12b"
      }
    },
    "android": {
      "package": "com.bblender70.chsschattanchalmobileapp",
      "adaptiveIcon": {
        "foregroundImage":"./imgs/icons/adaptive-icon.png",
        "backgroundColor":"#ffffff"
      }
    }
  }
}
